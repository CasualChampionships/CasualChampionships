{
  "schemaVersion": 1,
  "id": "uhc",
  "version": "${version}",
  "name": "Casual UHC Mod",
  "description": "UHC mod for Casual UHC",
  "authors": [
    "Sensei"
  ],
  "contact": {
    "repo": "https://github.com/CasualUHC/uhc-mod.git"
  },
  "icon": "icon.png",
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.casualuhc.uhcmod.UHCMod"
      }
    ]
  },
  "mixins": [
    "uhcmod.mixins.json"
  ],
  "accessWidener" : "uhcmod.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.12",
    "minecraft": "1.20.x",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.0+kotlin.1.8.0"
  }
}
